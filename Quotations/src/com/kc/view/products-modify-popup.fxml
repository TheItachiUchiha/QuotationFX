            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="70.0" text="Category" fx:id="category" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="82.0" text="SubCategory" fx:id="subcategory" />
